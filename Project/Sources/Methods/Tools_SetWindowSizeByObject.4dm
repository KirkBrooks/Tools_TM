//%attributes = {"shared":true}  // modify window size, based on Object $1  // if object empty, nothing is doneC_OBJECT:C1216($obj;$1)$obj:=$1If (Not:C34(OB Is empty:C1297($obj)))	$left:=OB Get:C1224($obj;"left")	$top:=OB Get:C1224($obj;"top")	$right:=OB Get:C1224($obj;"right")	$bottom:=OB Get:C1224($obj;"bottom")	SET WINDOW RECT:C444($left;$top;$right;$bottom)End if 