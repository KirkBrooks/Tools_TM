//%attributes = {"shared":true}  // Decnum := Tools_HexToDec(Hexstring)  // $decnum := Tools_HextoDec("A2b3")C_TEXT:C284($1;$thisPosition)C_LONGINT:C283($0;$temp)$temp:=0$length:=Length:C16($1)For ($a;1;$length)	$thisPosition:=Substring:C12($1;$a;1)	Case of 		: ($thisPosition="A")			$thisPosition:="10"		: ($thisPosition="B")			$thisPosition:="11"		: ($thisPosition="C")			$thisPosition:="12"		: ($thisPosition="D")			$thisPosition:="13"		: ($thisPosition="E")			$thisPosition:="14"		: ($thisPosition="F")			$thisPosition:="15"	End case 	$thisMultiplier:=16^($length-$a)	$temp:=$temp+(Num:C11($thisPosition)*($thisMultiplier))End for $0:=$temp